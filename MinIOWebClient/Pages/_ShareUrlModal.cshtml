@model MinIOWebClient.Models.ShareModel

<h5 class="mb-3">@Model.ObjectName</h5>
<label for="" class="form-label">Share URL:</label>
<textarea type="text" disabled="disabled" class="form-control" rows="5" asp-for="ShareUrl"></textarea>
<div class="alert alert-warning text-center mt-3 mb-0">
    Link expires in <strong>@Model.ExpireString</strong>!
</div>
